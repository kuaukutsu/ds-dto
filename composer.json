{
  "name": "kuaukutsu/ds-dto",
  "description": "Data Structure: DTO (Data Transfer Object)",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Dmitriy Krivopalov",
      "email": "dimarik82@gmail.com"
    }
  ],
  "support": {
    "issues": "https://github.com/kuaukutsu/ds-dto/issues",
    "source": "https://github.com/kuaukutsu/ds-dto"
  },
  "require": {
    "php": "^8.1",
    "yiisoft/arrays": "^3.0",
    "yiisoft/strings": "^2.3",
    "kuaukutsu/ds-collection": "^2.0.2"
  },
  "require-dev": {
    "vimeo/psalm": "^5.15",
    "friendsofphp/php-cs-fixer": "^3.13",
    "icanhazstring/composer-unused": "^0.8",
    "phpunit/phpunit": "^9.5",
    "rector/rector": "^0.18",
    "roave/infection-static-analysis-plugin": "^1.3",
    "roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^8.7",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "config": {
    "platform": {
      "php": "8.1"
    },
    "allow-plugins": {
      "infection/extension-installer": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "kuaukutsu\\ds\\dto\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "kuaukutsu\\ds\\dto\\tests\\": "tests"
    }
  }
}
